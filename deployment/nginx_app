server {
    listen 80;
    server_name _;

    location = /favicon.ico { access_log off; log_not_found off; }
    
    location /static/ {
        root /home/ubuntu/ReviewSystem;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/ubuntu/ReviewSystem/review_system.sock;
    }
}
